RewriteEngine on

RewriteRule ^next/users/?$     next/users.php [NC,L]
RewriteRule ^next/lang/en/?$   next/en.php [NC,L]
RewriteRule ^next/lang/es/?$   next/es.php [NC,L]
RewriteRule ^next/lang/zh/?$   next/zh.php [NC,L]
RewriteRule ^next/lang/he/?$   next/he.php [NC,L]
RewriteRule ^next/project/doc/?$       next/documentation.php [NC,L] 
RewriteRule ^users/?$     users.php [NC,L]
RewriteRule ^lang/en/?$   en.php [NC,L]
RewriteRule ^lang/es/?$   es.php [NC,L]
RewriteRule ^lang/zh/?$   zh.php [NC,L]
RewriteRule ^lang/he/?$   he.php [NC,L]
RewriteRule ^plain/lang/he/?$     plain.php?locale=he_IL [NC,L]
RewriteRule ^plain/lang/en/?$     plain.php?locale=en_US [NC,L]
RewriteRule ^plain/lang/es/?$     plain.php?locale=es_AR [NC,L]
RewriteRule ^plain/lang/zh/?$     plain.php?locale=zh_CN [NC,L]
RewriteRule ^project/doc/?$       documentation.php [NC,L] 
